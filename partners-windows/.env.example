# Clutch Partners Windows App Configuration
# Copy this file to .env and fill in the actual values

# API Configuration
REACT_APP_API_BASE_URL=https://api.clutch.com/v1

# Development Configuration
REACT_APP_DEBUG_MODE=false
REACT_APP_LOG_LEVEL=info

# Security Configuration
REACT_APP_ENABLE_DEV_TOOLS=false
REACT_APP_ENABLE_LOGGING=true

# Sync Configuration
REACT_APP_SYNC_INTERVAL=30000
REACT_APP_MAX_RETRIES=3
REACT_APP_OFFLINE_MODE=true

# Database Configuration
REACT_APP_DB_NAME=clutch_partners
REACT_APP_DB_VERSION=1

# Feature Flags
REACT_APP_ENABLE_BARCODE_SCANNER=true
REACT_APP_ENABLE_OFFLINE_SYNC=true
REACT_APP_ENABLE_REAL_TIME_UPDATES=true
REACT_APP_ENABLE_PUSH_NOTIFICATIONS=true

<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude sensitive data from cloud backup -->
        <exclude domain="sharedpref" path="clutch_secure_prefs.xml"/>
        <exclude domain="database" path="clutch_cache.db"/>
    </cloud-backup>
    <device-transfer>
        <!-- Exclude sensitive data from device transfer -->
        <exclude domain="sharedpref" path="clutch_secure_prefs.xml"/>
        <exclude domain="database" path="clutch_cache.db"/>
    </device-transfer>
</data-extraction-rules>

